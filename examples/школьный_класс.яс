функция округлить(число, вМеньшуюСторону) 
	константа [результат, дробоваяЧасть] = (""+число).разбить(".");
	если(!вМеньшуюСторону && ((1*дробоваяЧасть) >= 5) ) 
		вернуть (результат *1) + 1  
	вернуть результат * 1  

класс Класс 
	конструктор(учеников){
		тут.учеников = учеников
		тут.учительскийСтул = 1
		тут.учительскийСтол = 1
		тут.площадь = 300
		тут.площадьПарты = 10  
	получить максимальноеКоличествоПарт() 
		вернуть округлить(тут.площадь/тут.площадьПарты, 1)  
	получить парты() 
		вернуть округлить(тут.учеников/2)  
	получить всегоМест() 
		вернуть тут.парты * 2  
	получить стульев() 
		вернуть тут.всегоМест + тут.учительскийСтул  
	получить столы() 
		вернуть тут.парты + тут.учительскийСтол  
	получить свободныхМест() 
		вернуть тут.всегоМест - тут.учеников  


константа первыйКласс =  новый Класс(23)
консоль.лог(первыйКласс)